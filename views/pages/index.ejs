<div class="container">
  <h2>Todo List</h2>
  <form action="/addtodo" method="POST">
    <input type="text" name="newtodo" placeholder="create new todo" />
    <button>Add Todo</button>
    <h2>Added Todo</h2>
    <% for( let i = 0; i < todoList.length; i++){ %>
    <li>
      <input type="checkbox" name="check" value="<%= todoList[i] %>" /> <%=
      todoList[i] %>
    </li>
    <% } %>
    <button formaction="/removetodo" type="submit">Remove</button>
  </form>
  <h2>Completed todo</h2>
  <% for(let i = 0; i < completeList.length; i++){ %>
  <li><input type="checkbox" checked /><%= completeList[i] %></li>
  <% } %>
</div>
